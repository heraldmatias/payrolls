<table class="table table-hover table-bordered" id="tbl_data">
    <thead>
        <tr class="row-black">
        {% for column in columns %}
            <th>{{ column }}</th>
        {% endfor %}
        </tr>
    </thead>
    <tbody>
        {% for row in rows %}
        <tr {% if loop.index is odd %}class="info"{% endif %}>
            {% for cell in row %}
            <td>{{ cell }}</td>
            {% endfor %}
        </tr>
        {% endfor %}
    </tbody>
</table>
